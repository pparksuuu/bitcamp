<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- 프로퍼티 설정하기 = 셋터 호출하기 -->

	<bean id="c1" class="bitcamp.java106.step05.Car4" >
	    <!-- List 프로퍼티 값 설정하기 : 배열과 똑같다.-->
        <property name="options">
            <props>
                <prop key="sunroof">true</prop>
                <prop key="auto">true</prop>
                <prop key="jdbc.username">java106</prop>
                <prop key="jdbc.password">1111</prop>
            </props>
        </property>	
	</bean>
	
	<bean id="e1" class="bitcamp.java106.step05.Engine" p:maker="비트자동차"/>
	
	
	
</beans>



